# Google Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Application Settings
VOICE_ENABLED=true
DEFAULT_VOICE=0  # 0 for male, 1 for female
SPEECH_RATE=200  # Words per minute

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=jarvis.log

# System Settings
REQUIRE_ADMIN_FOR_SYSTEM_COMMANDS=true
DEFAULT_WEB_BROWSER=default  # Can be 'chrome', 'firefox', 'edge', or 'default'
